{
  "name": "my-react-starter",
  "version": "0.1.0",
  "description": "Common React + Redux starter pack",
  "scripts": {
    "dev": "webpack-dev-server --mode development --hot --progress --host 0.0.0.0 --port 8080",
    "prod": "npm run clean && webpack --config config/webpack.prod.js --mode production --progress --profile --bail",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hartex/my-react-starter.git"
  },
  "author": "hartex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hartex/my-react-starter/issues"
  },
  "homepage": "https://github.com/hartex/my-react-starter#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "core-js": "^2.5.5",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.1",
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
